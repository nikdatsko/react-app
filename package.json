{
  "name": "react-app",
  "version": "1.0.0",
  "description": "React app",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development  node ./server",
    "build": "babel src/ -d lib/",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore --fix",
    "prod": "cross-env NODE_ENV=production  node ./server",
    "build:dev":
      "rm -rf dist && webpack --mode development --config webpack.dev.config.js",
    "build:prod":
      "rm -rf dist && webpack --mode production --config webpack.prod.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "cypress": "cypress open",
    "flow": "flow",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikdatsko/react-app.git"
  },
  "keywords": ["react"],
