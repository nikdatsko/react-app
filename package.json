{
  "name": "react-app",
  "version": "1.0.0",
  "description": "React app",
  "main": "index.js",
  "scripts": {
    "start":
      "webpack-dev-server --open --mode development --config webpack.dev.config.js",
    "build": "webpack --mode production",
    "build:dev":
      "rm -rf dist && webpack --mode development --config webpack.dev.config.js",
    "build:prod":
      "rm -rf dist && webpack --mode production --config webpack.prod.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "cypress": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikdatsko/react-app.git"
  },
  "keywords": ["react"],
  "author": "Nik Datsko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nikdatsko/react-app/issues"
  },
  "homepage": "https://github.com/nikdatsko/react-app#readme",
  "dependencies": {
    "express": "^4.16.4",
    "jest": "^24.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-scripts": "^2.1.5",
    "redux": "^4.0.1"
  },
  "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"],
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prop-types": "^15.7.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
